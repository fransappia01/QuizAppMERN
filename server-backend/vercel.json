{
    "version": 2,
    "functions": {
        "api": {
            "/*": "api/*.js"
        }
      },
    "builds":[
        {
            "src": "*.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/ind.js"
        }
    ]
}